<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Quintonn Rothmann</Authors>
    <Company>Quintonn Rothmann</Company>
    <Copyright>Copyright �  2018</Copyright>
    <Version>5.0.0</Version>
    <RepositoryUrl>https://github.com/quintonn/QBic</RepositoryUrl>
    <Product>QBic.FrontEnd</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <IncludeSymbols>false</IncludeSymbols>
    <NoPackageAnalysis>false</NoPackageAnalysis>
  </PropertyGroup>

  

  <ItemGroup>
    <None Include="wwwroot/**/*" Pack="True" PackagePath="content/wwwroot" />
    <None Remove="package.json" />
  </ItemGroup>
</Project>