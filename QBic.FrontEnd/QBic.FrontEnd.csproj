<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <PackageId>QBic.FrontEnd</PackageId>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Quintonn Rothmann</Authors>
    <Company>Quintonn Rothmann</Company>
    <Copyright>Copyright �  2018</Copyright>
    <Version>5.0.0.22</Version>
    <RepositoryUrl>https://github.com/quintonn/QBic</RepositoryUrl>
    <Product>QBic.FrontEnd</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <_ContentIncludedByDefault Remove="readme2.txt" />
  </ItemGroup>

  <ItemGroup>
    <None Include="readme.txt" Pack="true" PackagePage=".">
    </None>
  </ItemGroup>

  <ItemGroup>
    <Content Include="readme2.txt">
      <PackageFlatten>true</PackageFlatten>
      <Pack>true</Pack>
    </Content>
  </ItemGroup>

</Project>