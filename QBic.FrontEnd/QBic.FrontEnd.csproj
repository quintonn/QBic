<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Quintonn Rothmann</Authors>
    <Company>Quintonn Rothmann</Company>
    <Copyright>Copyright �  2018</Copyright>
    <Version>5.0.0</Version>
    <RepositoryUrl>https://github.com/quintonn/QBic</RepositoryUrl>
    <Product>QBic.FrontEnd</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
    <IncludeBuildOutput>false</IncludeBuildOutput>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;NU5128;NU5017</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Include="wwwroot/**/*" Pack="True" PackagePath="content/wwwroot" />
    <None Remove="package.json" />
  </ItemGroup>
</Project>