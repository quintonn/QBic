<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <PackageId>QBic.FrontEnd</PackageId>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Quintonn Rothmann</Authors>
    <Company>Quintonn Rothmann</Company>
    <Copyright>Copyright �  2018</Copyright>
    <Version>5.0.0.6</Version>
    <RepositoryUrl>https://github.com/quintonn/QBic</RepositoryUrl>
    <Product>QBic.FrontEnd</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <NoPackageAnalysis>true</NoPackageAnalysis>
    <EnableDefaultItems>false</EnableDefaultItems>
  </PropertyGroup>

  <!--<ItemGroup>
    <None Include="wwwroot/**/*" Pack="True" PackagePath="content/wwwroot" />
    <Content Remove="package*.json" />
  </ItemGroup>-->
  <ItemGroup>
    <Content Include="wwwroot/**/*" Pack="True" />
    <None Include="*.*"  Pack="False"/>
  </ItemGroup>
</Project>